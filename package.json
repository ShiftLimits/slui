{
  "name": "@shiftlimits/ui",
  "version": "0.0.3",
  "license": "MIT",
  "author": "Jeff Schofield <jeff@jeffschofield.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShiftLimits/slui.git"
  },
  "bugs": {
    "url": "https://github.com/ShiftLimits/slui/issues",
    "email": "jeff@jeffschofield.com"
  },
  "keywords": [
    "vue",
    "tailwind",
    "ui",
    "library"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./tailwind": {
      "import": "./dist/tailwind.es.js",
      "require": "./dist/tailwind.umd.js",
      "types": "./dist/tailwind.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.es.js",
      "require": "./dist/vite.umd.js",
      "types": "./dist/vite.d.ts"
    }
  },
  "files": [
    "dist",
    "assets",
    "colors.config.js",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "vitest",
    "format": "eslint --fix \"**/*.{js,ts,vue}\"",
    "build": "rimraf ./dist && rimraf ./assets && run-p build:* && copyfiles -u 2 src/assets/**/* assets",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "build:library": "vite build",
    "build:tailwind": "vite build --config vite.config.tailwind.ts",
    "build:vite": "vite build --config vite.config.vite.ts"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "esno scripts/verify-commit.ts"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "lodash.merge": "^4.6.2",
    "tailwindcss-color-suite": "^0.7.0",
    "tailwindcss-gridlines": "^0.2.4",
    "tailwindcss-interaction-media": "^0.1.0",
    "tailwindcss-leading-trim": "^1.1.0",
    "tailwindcss-rounded-inherit": "^0.1.0",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "devDependencies": {
    "@floating-ui/core": "^1.0.1",
    "@headlessui-float/vue": "^0.9.1",
    "@headlessui/vue": "^1.7.2",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@urql/devtools": "^2.0.3",
    "@urql/vue": "^1.0.2",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.12",
    "copyfiles": "^2.4.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "esno": "^0.16.3",
    "graphql": "^16.6.0",
    "jest-matcher-css": "^1.1.0",
    "kolorist": "^1.6.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.23.4",
    "vue": "^3.2.39",
    "vue-eslint-parser": "^9.1.0",
    "vue-router": "^4.1.5",
    "vue-tsc": "^0.40.13",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "@headlessui-float/vue": "^0.7.0",
    "@headlessui/vue": "^1.6.0",
    "@urql/vue": "^0.6.0",
    "tailwindcss": "^3.0.0",
    "vite": "^3.0.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@headlessui-float/vue": {
      "optional": true
    },
    "@headlessui/vue": {
      "optional": true
    },
    "@urql/vue": {
      "optional": true
    },
    "vue-router": {
      "optional": true
    }
  }
}
